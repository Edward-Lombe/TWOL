extends layout

block content
  div(class='eight columns offset-by-four', ng-controller='projectControl')
    h1(style='text-align:center;')
      | Projects
    article(ng-repeat='project in projects')
      h2
        | {{project.title}}
      h3(class='subheader')
        | {{project.date}}
      p
        | {{project.content}}
      hr
      br

